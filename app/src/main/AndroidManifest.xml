<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nelson.mp3fly"
    android:versionCode="8"
    android:versionName="1.8">
    <permission android:name="com.example.nelson.mp3fly.permission.DEADLY_ACTIVITY"

        android:permissionGroup="android.permission-group.STORAGE"
        android:label="Teste"
        android:protectionLevel="normal" />



    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_DOWNLOAD_MANAGER" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.EXTRA_STORAGE_VOLUME"/>
    <uses-permission android:name="android.permission."/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/mp3l2"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Tocador"
            android:configChanges="orientation|screenSize"
            android:launchMode="singleTop"
            android:finishOnTaskLaunch="true"
            android:multiprocess="false">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>


</manifest>
